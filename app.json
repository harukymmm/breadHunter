{
  "expo": {
    "name": "breadHunter",
    "slug": "breadHunter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/breadicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/breadicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
       },
    "plugins": [
        [
          "expo-font",
          {
            "fonts":["./assets/fonts/SmileySans-Oblique.otf"]
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/breadicon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }

